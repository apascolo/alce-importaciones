    "firebase-functions": "^4.3.1",
    "fs-extra": "^10.0.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "sharp": "^0.33.3",
    "validator": "^13.11.0"
  },
